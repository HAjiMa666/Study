{
  "workbench.colorTheme": "One Dark Pro Darker",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "workbench.iconTheme": "vscode-icons",
  "editor.linkedEditing": true,
  "editor.fontSize": 18,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {},
  "emmet.triggerExpansionOnTab": true,
  //激活彩虹括号
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "liveSassCompile.settings.includeItems": [],
  //JS参数名称和类型的内联提示
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.variableTypes.enabled": false,
  "diffEditor.wordWrap": "off",
  "settingsSync.ignoredExtensions": [],
  "gitlens.defaultDateLocale": null,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "backgroundCover.imagePath": "d:\\BaiduNetdiskWorkspace\\images\\壁纸\\wallhaven-8o1ejo.png",
  "backgroundCover.opacity": 0.5,

  // 头部注释
  "fileheader.customMade": {
    "Author": "czx",
    "Date": "Do not edit", // 设置后默认设置文件生成时间
    "LastEditTime": "Do not edit", // 设置后，保存文件更改默认更新最后编辑时间
    "LastEditors": "czx", // 设置后，保存文件更改默认更新最后编辑人
    "Description": ""
  },
  "fileheader.configObj": {
    "prohibitAutoAdd": ["json", "html", "css", "scss", "less", "md"]
  },
  "gitlens.views.branches.branches.layout": "list",
  "cSpell.languageSettings": [],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": ":",
      "color": "#e2d849",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.jsxBracketSameLine": true,
  "prettier.proseWrap": "always",
  "gitlens.codeLens.dateFormat": "",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "gitlens.currentLine.dateFormat": "",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": ["antd"],
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.folders.theme": "classic",
  "workbench.sideBar.location": "left"
}
